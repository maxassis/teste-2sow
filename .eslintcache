tatus === 200 &&\n          response.data.erro === undefined\n        ) {\n          document.getElementById(\"cidade\").value = response.data.localidade;\n          document.getElementById(\"bairro\").value = response.data.bairro;\n          const rua = (document.getElementById(\"rua\").value =\n            response.data.logradouro);\n          rua !== undefined && executePost();\n        } else if (response.data.erro === true) {\n          alert(\"deu ruim\");\n        }\n      })\n      .catch((error) => {\n        alert(\"deu erro\");\n        console.log(error.response);\n      });\n\n    const executePost = async () => {\n      const body = {\n        id: uuidv4(),\n        nome: data.nome,\n        cpf: data.cpf,\n        email: data.email,\n        endereco: {\n          cep: data.cep,\n          rua: document.getElementById(\"rua\").value,\n          numero: data.numero,\n          bairro: document.getElementById(\"bairro\").value,\n          cidade: document.getElementById(\"cidade\").value,\n        },\n      };\n\n      const response = await axios.post(\"http://localhost:5000/usuarios\", body);\n\n      console.log(response);\n    };\n  }\n\n  return (\n    <>\n      <Header menu1={\"Lista\"} menu2={\"Busca\"} menu3={\"Logout\"} />\n      <S.WrapperTitle>\n        <h1>Cadastro</h1>\n      </S.WrapperTitle>\n      <S.WrapperForm>\n        <S.Frm onSubmit={handleSubmit(onSubmit)}>\n          <S.Input name=\"nome\" ref={register()} placeholder=\"Nome\" />\n          {errors.nome && <S.Span>O nome e obrigatorio</S.Span>}\n          <Controller\n            as={S.InputMsk}\n            control={control}\n            mask=\"999.999.999-99\"\n            name=\"cpf\"\n            maskPlaceholder={null}\n            placeholder=\"Cpf\"\n            defaultValue=\"\"\n          />\n          {errors.cpf && <S.Span>O cpf e obrigatorio</S.Span>}\n          <S.Input name=\"email\" ref={register()} placeholder=\"Email\" />\n          {errors.email && <S.Span>Digite um email valido</S.Span>}\n          <div>\n            <Controller\n              as={S.InputMsk}\n              control={control}\n              mask=\"99999-999\"\n              maskPlaceholder={null}\n              name=\"cep\"\n              placeholder=\"Cep\"\n              defaultValue=\"\"\n            />\n            {errors.cep && <S.Span>O cep e obrigatorio</S.Span>}\n          </div>\n          <S.Input\n            name=\"numero\"\n            type=\"number\"\n            ref={register()}\n            placeholder=\"Numero\"\n          />\n          {errors.numero && <S.Span>O numero e obrigatorio</S.Span>}\n          <S.Input\n            name=\"rua\"\n            id=\"rua\"\n            ref={register()}\n            placeholder=\"Rua\"\n            readOnly=\"true\"\n          />\n          {errors.rua && <S.Span>A rua e obrigatorio</S.Span>}\n          <S.Input\n            name=\"bairro\"\n            id=\"bairro\"\n            ref={register()}\n            placeholder=\"Bairro\"\n            readOnly=\"true\"\n          />\n          {errors.bairro && <S.Span>O bairro e obrigatorio</S.Span>}\n          <S.Input\n            name=\"cidade\"\n            id=\"cidade\"\n            ref={register()}\n            placeholder=\"Cidade\"\n            readOnly=\"true\"\n          />\n          {errors.cidade && <S.Span>A cidade e obrigatorio</S.Span>}\n          <S.Btn>Enviar</S.Btn>\n        </S.Frm>\n      </S.WrapperForm>\n    </>\n  );\n}\n","/home/max/Área de Trabalho/Projetos/teste-2sow/src/pages/Register/styles.js",[],"/home/max/Área de Trabalho/Projetos/teste-2sow/src/index.js",[],"/home/max/Área de Trabalho/Projetos/teste-2sow/src/components/header/styles.js",[],"/home/max/Área de Trabalho/Projetos/teste-2sow/src/components/header/index.js",[],["72","73"],"/home/max/Área de Trabalho/Projetos/teste-2sow/src/styles/global.js",[],"/home/max/Área de Trabalho/Projetos/teste-2sow/src/pages/List/index.js",[],"/home/max/Área de Trabalho/Projetos/teste-2so